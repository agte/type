{
  "name": "@agte/type",
  "version": "0.0.1",
  "description": "Simple type check library",
  "main": "index.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "eslint --fix .",
    "test": "ava test/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agte/type.git"
  },
  "keywords": [
    "type",
    "check",
    "validation"
  ],
  "author": {
    "name": "Anna Agte",
    "email": "matiasrustagte@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agte/type/issues"
  },
  "homepage": "https://github.com/agte/type#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "esm": "^3.2.25"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "type": "module",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "airbnb-base",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages"
      ]
    }
  },
  "ava": {
    "require": [
      "esm"
    ],
    "testOptions": {
      "presets": [
        [
          "module:ava/stage-4",
          {
            "modules": false
          }
        ]
      ]
    },
    "babel": false,
    "compileEnhancements": false
  }
}
