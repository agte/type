    "type": "git",
    "url": "git+https://github.com/agte/type.git"
  },
  "keywords": [
    "type",
    "check",
    "validation"
  ],
  "author": "Anna Agte <matiasrustagte@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agte/type/issues"
  },
  "homepage": "https://github.com/agte/type#readme",
  "devDependencies": {
